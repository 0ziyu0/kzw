package com.kzw.vo;

import com.kzw.pojo.TbPartner;
import com.kzw.pojo.TbUser;

public class PartnerPo {

	/**
	 * 合伙人 
	 */
	private TbPartner partner;
	/**
	 * 保存当前合伙人的信息（名字，地址，电话等）
	 */
	private TbUser user;
	/**
	 *判断当前人的身份  *店主 *合伙人
	 */
	private String status;
	public TbPartner getPartner() {
		return partner;
	}
	public void setPartner(TbPartner partner) {
		this.partner = partner;
	}
	public TbUser getUser() {
		return user;
	}
	public void setUser(TbUser user) {
		this.user = user;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public PartnerPo() {
		super();
	}
	public PartnerPo(TbPartner partner, TbUser user, String status) {
		super();
		this.partner = partner;
		this.user = user;
		this.status = status;
	}
	
}
